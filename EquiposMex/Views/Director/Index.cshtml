@model IEnumerable<EquiposMex.Models.DIRECTOR>

@{
    ViewBag.Title = "Index";
}

<h2>LISTA DE DIRECTORES</h2>

<p>
    @Html.ActionLink("Nuevo director", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID_DIRECTOR)
        </th>
        <th>
            NOMBRE EQUIPO
            @*@Html.DisplayNameFor(model => model.EQUIPO.NOMBREEQUIPO)*@
        </th>
        <th>
            NOMBRE DIRECTOR
            @*@Html.DisplayNameFor(model => model.NOMBREDIRECTOR)*@
        </th>
        <th>
            APELLIDOS 
            @*@Html.DisplayNameFor(model => model.APELLIDOSDIRECTOR)*@
        </th>
        <th>
            TELEFONO
            @*@Html.DisplayNameFor(model => model.TELEFONODIRECTOR)*@
        </th>
        <th colspan ="3">OPCIONES</th>
</tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID_DIRECTOR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EQUIPO.NOMBREEQUIPO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NOMBREDIRECTOR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.APELLIDOSDIRECTOR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TELEFONODIRECTOR)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ID_DIRECTOR })
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.ID_DIRECTOR })
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id = item.ID_DIRECTOR })
        </td>
    </tr>
}

</table>
